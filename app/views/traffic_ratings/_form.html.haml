.grey-form
  = form_for @traffic_rating do |f|
    = f.error_messages
    %p
      = f.label :rating
      %br
      = f.text_field :rating
    %p
      = f.label :dsescription
      %br
      = f.text_field :dsescription
    %p
      = f.label :icon_path
      %br
      = f.text_field :icon_path
    %p
      = submit_button('Submit')
